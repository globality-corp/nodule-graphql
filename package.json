{
    "name": "@globality/nodule-graphql",
    "version": "2.1.0",
    "description": "Node GraphQL Conventions",
    "main": "lib/",
    "repository": "git@github.com:globality-corp/nodule-graphql",
    "author": "Globality Engineering",
    "license": "MIT",
    "scripts": {
        "clean": "rm -rf ./lib",
        "build": "babel src --out-dir lib --ignore '**/__tests__/*,**/__mocks__/*'",
        "lint": "gi-eslint-run lint",
        "lint:fix": "gi-eslint-run lint --fix",
        "pack:dev": "./scripts/pack-dev.sh",
        "test": "jest",
        "verify": "yarn lint && yarn test",
        "preyalc": "yarn clean && yarn build"
    },
    "dependencies": {
        "axios": "0.26.1",
        "connect-requestid": "1.1.0",
        "cors": "2.8.5",
        "dataloader": "1.4.0",
        "enum": "2.5.0",
        "express": "4.17.1",
        "express-jwt": "6.0.0",
        "graphql-playground-middleware-express": "1.7.22",
        "helmet": "3.23.3",
        "http-status-codes": "2.1.4",
        "is-uuid": "1.0.2",
        "jsonwebtoken": "8.5.1",
        "lodash": "4.17.21",
        "spookyhash": "2.0.0",
        "uuid": "8.3.2"
    },
    "peerDependencies": {
        "@globality/nodule-config": ">= 2.13.3 < 3",
        "@globality/nodule-express": ">= 0.1.14 < 1",
        "@globality/nodule-logging": ">= 1.5.2 < 2",
        "@globality/nodule-openapi": ">= 0.21.0 < 1",
        "graphql": "^15.5.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.17.6",
        "@babel/core": "^7.17.8",
        "@babel/node": "^7.16.8",
        "@babel/preset-env": "^7.16.11",
        "@globality/gi-eslint-config": "^1.0.12",
        "@globality/nodule-config": "2.13.3",
        "@globality/nodule-express": "0.1.14",
        "@globality/nodule-logging": "1.5.2",
        "@globality/nodule-openapi": "0.21.0",
        "body-parser": "1.19.0",
        "graphql": "^15.5.0",
        "jest": "^27.5.1",
        "supertest": "6.1.2"
    },
    "resolutions": {
        "lodash": "^4.17.21"
    },
    "jest": {
        "testRegex": "/__tests__/.*(test)\\.js",
        "modulePaths": [
            "<rootDir>/src"
        ],
        "testURL": "http://localhost"
    }
}
